#include <stdio.h>

int main() {
    int n, x;

  
    printf("Enter number of elements in array: ");
    scanf("%d", &n);

    int arr[n];

    
    printf("Enter %d elements in sorted order:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Enter x: ");
    scanf("%d", &x);

    int ceilIndex = -1;

    
    for (int i = 0; i < n; i++) {
        if (arr[i] >= x) {
            ceilIndex = i;
            break;
        }
    }

    printf("Index of ceil: %d\n", ceilIndex);

    return 0;
}
