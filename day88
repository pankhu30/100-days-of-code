#include <stdio.h>

int main() {
    enum UserRole { ADMIN = 10, USER = 20, GUEST = 30 };

  
    char *roleNames[] = { "ADMIN", "USER", "GUEST" };

  
    int roleValues[] = { ADMIN, USER, GUEST };

    int size = sizeof(roleValues) / sizeof(roleValues[0]);

    printf("Enum names and their integer values:\n");
    for (int i = 0; i < size; i++) {
        printf("%s = %d\n", roleNames[i], roleValues[i]);
    }

    return 0;
}
