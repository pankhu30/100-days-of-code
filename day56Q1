#include <stdio.h>

int main() {
    int n;

    
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n], nextGreater[n];

  
    printf("Enter %d elements:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    
    for (int i = 0; i < n; i++) {
        nextGreater[i] = -1; 
        for (int j = i + 1; j < n; j++) {
            if (arr[j] > arr[i]) {
                nextGreater[i] = arr[j];
                break; 
            }
        }
    }


    printf("Next greater elements: ");
    for (int i = 0; i < n; i++) {
        if (i != 0) printf(", ");
        printf("%d", nextGreater[i]);
    }
    printf("\n");

    return 0;
}
