#include <stdio.h>

int main() {
    long num;
    int digitCount[10] = {0}; 

    
    printf("Enter an integer number: ");
    scanf("%ld", &num);

    if (num < 0) {
        num = -num; 
    }

    
    while (num != 0) {
        int digit = num % 10;
        digitCount[digit]++;
        num /= 10;
    }

    
    int maxCount = 0, mostDigit = 0;
    for (int i = 0; i < 10; i++) {
        if (digitCount[i] > maxCount) {
            maxCount = digitCount[i];
            mostDigit = i;
        }
    }

  
    printf("Digit %d occurs the most (%d times).\n", mostDigit, maxCount);

    return 0;
}
